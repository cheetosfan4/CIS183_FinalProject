<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff4df">

    <TextView
        android:id="@+id/tv_v_simplePaletteCell_ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:paddingBottom="4dp"
        android:text="ID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_v_simplePaletteCell_display1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="2dp"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_v_simplePaletteCell_display2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="2dp"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toEndOf="@+id/view_v_simplePaletteCell_display1"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_v_simplePaletteCell_display3"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="2dp"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toEndOf="@+id/view_v_simplePaletteCell_display2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_v_simplePaletteCell_display4"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="2dp"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toEndOf="@+id/view_v_simplePaletteCell_display3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_v_simplePaletteCell_ellipses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:paddingBottom="0dp"
        android:text=". . ."
        android:textAlignment="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/view_v_simplePaletteCell_display4"
        app:layout_constraintStart_toEndOf="@+id/view_v_simplePaletteCell_display4" />
</androidx.constraintlayout.widget.ConstraintLayout>